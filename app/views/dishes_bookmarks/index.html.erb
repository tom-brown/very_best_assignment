<div class="page-header">
  <h1>
    Dishes bookmarks
    <a href="/dishes_bookmarks/new" class="btn btn-lg btn-success">New Dishes bookmark</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Dish</th>
        <th>Bookmark</th>
        <th>Actions</th>
      </tr>

      <% @dishes_bookmarks.each do |dishes_bookmark| %>
      <tr>
        <td>
          <% if dishes_bookmark.dish.present? %>
            <a href="/dishes/<%= dishes_bookmark.dish.id %>">
              <%= dishes_bookmark.dish.name %>
            </a>
          <% end %>
        </td>
        <td>
          <% if dishes_bookmark.bookmark.present? %>
            <a href="/bookmarks/<%= dishes_bookmark.bookmark.id %>">
              <%= dishes_bookmark.bookmark.venue_id %>
            </a>
          <% end %>
        </td>
        <td>
          <a href="/dishes_bookmarks/<%= dishes_bookmark.id %>" class="btn btn-primary">Show</a>
          <a href="/dishes_bookmarks/<%= dishes_bookmark.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_dishes_bookmark/<%= dishes_bookmark.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @dishes_bookmarks, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
